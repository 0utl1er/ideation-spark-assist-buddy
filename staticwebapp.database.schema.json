{
  "$schema": "https://staticwebapps.blob.core.windows.net/database/schema/v1.0.0.json",
  "databaseId": "energy-data",
  "collections": {
    "users": {
      "fields": {
        "id": "string",
        "email": "string",
        "created_at": "string"
      }
    },
    "usage_logs": {
      "fields": {
        "id": "string",
        "user_id": "string",
        "timestamp": "string",
        "energy_used_kwh": "number"
      }
    },
    "utilities": {
      "fields": {
        "id": "string",
        "name": "string",
        "rate_plan": "string"
      }
    },
    "device_profiles": {
      "fields": {
        "id": "string",
        "user_id": "string",
        "device_name": "string",
        "estimated_kwh": "number"
      }
    },
    "ai_feedback_loop": {
      "fields": {
        "id": "string",
        "user_id": "string",
        "feedback": "string",
        "timestamp": "string"
      }
    },
    "energy_education": {
      "fields": {
        "id": "string",
        "title": "string",
        "category": "string",
        "url": "string"
      }
    }
  }
}
